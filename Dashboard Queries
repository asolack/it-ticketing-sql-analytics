SELECT * FROM it_reports.v_dashboard_summary;

SELECT *
FROM it_reports.mv_sla_by_priority
ORDER BY sla_breach_percent DESC;

SELECT *
FROM it_reports.mv_technician_scorecard
ORDER BY breach_percent DESC, resolved_tickets DESC;

SELECT *
FROM it_reports.mv_ticket_lifecycle
WHERE status IN ('Open','In Progress')
ORDER BY hours_open_now DESC
LIMIT 10;
